{
    "version": "1.1.0",
    "name": "QT Dialogue Bubble",
    "assetPath": "dialogue/qtDialogue/speech_bubble",
    "isPixel": false,
    "flipX": false,
    "flipY": false,
    "offsets": [-600, 20],
    "scale": 1.0,

    "text": {
        "offsets": [90, 120],
        "width": 1000,
        "color": "#3F2021",
        "fontFamily": "Pixel Arial 11 Bold",
        "shadowColor": "#D89494",
        "shadowWidth": 2
    },

    "animations": [
        {
            "name": "enter",
            "prefix": "speech bubble normal0",
            "offsets": [0, 0],
            "frameRate": 24
        },
        {
            "name": "right",
            "prefix": "speech bubble normal0",
            "offsets": [0, 0],
            "looped": true
        },
        {
            "name": "left",
            "prefix": "speech bubble left0",
            "offsets": [0, 0],
            "looped": true
        },
                {
            "name": "middle",
            "prefix": "speech bubble middle0",
            "offsets": [0, 0],
            "looped": true
        }
    ]
}