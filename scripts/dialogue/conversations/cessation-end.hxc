import funkin.play.cutscene.dialogue.Conversation;
import flixel.sound.FlxSound;
import funkin.play.PlayState;
import flixel.FlxG;

class CessationEndingConversation extends Conversation
{
    function new()
    {
        super('cessation-end');
    }

    override public function onDialogueEnd(event:DialogueScriptEvent):Void {
        super.onDialogueEnd(event);
        PlayState.instance.currentSong.scriptCall("endScreenHazard");
    }
}
